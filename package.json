{
  "name": "fweb3-api",
  "version": "1.0.0",
  "description": "Api behind the fweb3 ecosystem",
  "homepage": "https://github.com/fweb3/api#readme",
  "keywords": [
    "fweb3",
    "blockchain",
    "game"
  ],
  "author": "archae0pteryx (https://github.com/archae0pteryx)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fweb3/api/issues",
    "email": "dont@email.me"
  },
  "main": "dist/index",
  "typings": "dist/index",
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:clean": "rm -rf dist && npm run build",
    "lint": "eslint .",
    "pretty": "prettier --check .",
    "pretty:fix": "prettier -w .",
    "start": "node ./dist/app.js",
    "dev": "nodemon -r dotenv/config server/app.ts",
    "test": "jest",
    "test:ci": "jest",
    "test:watch": "jest --watchAll",
    "prepare": "husky install",
    "typegen": "npx typechain --target=ethers-v5 server/faucet/contracts/abi/*.json",
    "sync:local": "npx ts-node -O '{\"module\":\"commonjs\"}' scripts/sync.ts localhost",
    "sync:mumbai": "npx ts-node -O '{\"module\":\"commonjs\"}' scripts/sync.ts mumbai"
  },
  "devDependencies": {
    "@typechain/ethers-v5": "^10.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.25",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "dotenv": "^16.0.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.0",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.2",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typechain": "^8.0.0",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "discord-interactions": "^3.2.0",
    "ethers": "^5.6.4",
    "express": "^4.17.3",
    "helmet": "^5.0.2",
    "install": "^0.13.0",
    "morgan": "^1.10.0",
    "node-fetch": "^2.6.7",
    "npm": "^8.9.0"
  }
}
